{
	"name": "Bouquet",
	"version": "1.2.3",
	"author": [
		"[http://automattic.com Automattic]",
		"Jack Phoenix"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:Bouquet",
	"description": "Elegant, simple theme inspired by the beauty found in flowers",
	"license-name": "GPL-2.0+",
	"type": "skin",
	"ValidSkinNames": {
		"bouquet": "Bouquet"
	},
	"MessagesDirs": {
		"SkinBouquet": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinBouquet": "Bouquet.skin.php",
		"BouquetTemplate": "Bouquet.skin.php",
		"BouquetSkinNavigationService": "Bouquet.skin.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Bouquet"
	},
	"ResourceModules": {
		"skins.bouquet": {
			"styles": {
				"resources/print.css": {
					"media": "print"
				},
				"resources/style.css": {
					"media": "screen"
				},
				"resources/responsive/smallest.css": {
					"media": "(max-width: 450px)"
				},
				"resources/responsive/smaller.css": {
					"media": "(max-width: 650px)"
				},
				"resources/responsive/tiny.css": {
					"media": "screen and (max-width: 600px)"
				},
				"resources/responsive/small.css": {
					"media": "(max-width: 860px)"
				},
				"resources/responsive/medium.css": {
					"media": "(max-width: 1140px)"
				}
			},
			"position": "top"
		},
		"skins.bouquet.js": {
			"scripts": "resources/js/navigation.js"
		},
		"themeloader.skins.bouquet.forgetmenot": {
			"styles": {
				"resources/colors/forget-me-not/forget-me-not.css": {
					"media": "screen"
				}
			}
		},
		"themeloader.skins.bouquet.pinkdogwood": {
			"styles": {
				"resources/colors/pink-dogwood/pink-dogwood.css": {
					"media": "screen"
				}
			}
		},
		"themeloader.skins.bouquet.tigerlily": {
			"styles": {
				"resources/colors/tiger-lily/tiger-lily.css": {
					"media": "screen"
				}
			}
		}
	},
	"manifest_version": 1
}
